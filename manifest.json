{
  "manifest_version": 3,
  "name": "HTML Extractor",
  "version": "1.0",
  "description": "Extract and send HTML structure over WebSocket",
  "permissions": [
    "activeTab", "webRequest", "background"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://kassa.gbvweco.nl/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "extrastyling.css"
      ]
    }
  ],

  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}